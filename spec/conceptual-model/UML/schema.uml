classDiagram

   class AttributeDescriptor {
     name: String?
     title: String?
     description: String?
     mimeType: String?
     constraintName: String?
   }
   class Constraint {
     description: String?
   }
   class RangeConstraint {
     min: Numeric
     minIsInclusive: Boolean
     max: Numeric
     maxIsInclusive: Boolean
   }
   class GlobConstraint {
     glob: String
   }
   class EnumConstraint {
     value: String
   }
   Constraint --> AttributeDescriptor: constraintName
   Constraint <|-- RangeConstraint
   Constraint <|-- GlobConstraint
   Constraint <|-- EnumConstraint
   AttributeDescriptor --> EntityStore: entityStore
   AttributeDescriptor --> AttributeType: attributeType
   GeoPackage o-- "*" Constraint
   GeoPackage o-- "*" AttributeDescriptor
   EntityStore o-- "*" AttributeType
   